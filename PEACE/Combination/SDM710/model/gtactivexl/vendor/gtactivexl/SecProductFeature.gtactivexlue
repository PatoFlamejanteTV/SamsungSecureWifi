
# Feature naming rule
#
#	format)
#	SEC_PRODUCT_FEATURE_[Module name]_[Function name]=[Values]
#
#	- Feature name must be capital letter.
#	- Values : BOOLEAN (TRUE/FALSE) or STRING
#	ex)
#		Module name - WLAN
#		Function name - DISABLE_5G
#		Values - TRUE/FALSE (default : FALSE)
#
#	SEC_PRODUCT_FEATURE_WLAN_DISABLE_5G=FALSE
#
#	ex)
#		Module name - RIL
#		Function name - MMS_UAP_TAG_NAME
#		Values - x-wap-profile
#
#	SEC_PRODUCT_FEATURE_RIL_MMS_UAP_TAG_NAME="x-wap-profile"
#
source vendor/samsung/configs/gtactivexl_common/SecProductFeature.common

#AudioCore
SEC_PRODUCT_FEATURE_AUDIO_SET_DEFAULTSTREAM_NOTIFICATION=TRUE

# Global config for eSE (embedded Secure Element)
SEC_PRODUCT_FEATURE_SECURITY_CONFIG_ESE_CHIP_VENDOR="NXP"
SEC_PRODUCT_FEATURE_SECURITY_CONFIG_ESE_COS_NAME="JCOP4.0"

# Secure Wi-Fi
SEC_FLOATING_FEATURE_WLAN_SUPPORT_SECURE_WIFI=TRUE
