#Settings
SEC_FLOATING_FEATURE_SETTINGS_CONFIG_FCC_ID="A3LSMA6050"
SEC_FLOATING_FEATURE_SETTINGS_CONFIG_ELECTRIC_RATED_VALUE="DC 5 V; 1.55 A"
SEC_FLOATING_FEATURE_SETTINGS_CONFIG_BRAND_NAME="Galaxy A9 Star Lite"

# AUTOPOWER ON-OFF
SEC_FLOATING_FEATURE_SETTINGS_SUPPORT_AUTO_POWER_ON_OFF=TRUE

# FM Radio
SEC_FLOATING_FEATURE_FMRADIO_CONFIG_CHIP_VENDOR="4"
SEC_FLOATING_FEATURE_FMRADIO_REMOVE_AF_MENU=TRUE
SEC_FLOATING_FEATURE_FMRADIO_CONFIG_QUALCOMM_BLEND_SINR_HI="25"

#MODULE - SMARTMANAGER CHN 
SEC_FLOATING_FEATURE_SMARTMANAGER_CONFIG_PACKAGE_NAME="com.samsung.android.sm_cn" 
SEC_FLOATING_FEATURE_SECURITY_CONFIG_DEVICEMONITOR_PACKAGE_NAME="com.samsung.android.sm.devicesecurity.tcm"

# MODULE - LAUNCHER
SEC_FLOATING_FEATURE_LAUNCHER_CONFIG_ANIMATION_TYPE="LowestEnd"

# MODULE - SIP
SEC_FLOATING_FEATURE_SIP_ENABLE_DEF_KEY_SOUND=FALSE
SEC_FLOATING_FEATURE_COMMON_SUPPORT_NFC_HW_KEYBOARD=FALSE
